void setup() 
{ 
  size (600, 600);
  stroke (0);
  int x=int(random (600));
  int y=int(random (600));
  int w=int(random (300));
  int l=int(random (300));
  int r=int(random (255));
  int g=int(random (255));
  int b=int(random (255));
  int number=int(random (250));
  while (number!=100)
  {
    paint (x,y,w,l,r,g,b);
    number=int(random (250));
    x=int (random (600));
    y=int(random (600));
    w=int(random (300));
    l=int(random (300));
    r=int(random (255));
    g=int(random (255));
    b=int(random (255));
  }
}

void paint(int x, int y, int w, int l, int r, int g, int b)
{ 
  rect (x, y, w, l);
  fill(r, g, b );
}

